pipeline {
    
    agent any

    environment {
        CI = 'true'
        node = "D:\node"
        npm = "D:\node"
    }


    stages {
        stage('Build') { 
            steps {
                bat 'npm install'
            }
        }

        stage('Deploy') { 
            steps {
                bat 'npm start' 
            }
        }
    }
}
